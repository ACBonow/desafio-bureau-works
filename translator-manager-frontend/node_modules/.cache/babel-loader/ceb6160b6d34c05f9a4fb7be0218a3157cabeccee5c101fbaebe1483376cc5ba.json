{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'DocumentView',\n  data() {\n    return {\n      documents: [],\n      showModal: false,\n      editingDocument: {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      }\n    };\n  },\n  created() {\n    this.loadDocuments();\n  },\n  methods: {\n    async loadDocuments() {\n      try {\n        const response = await axios.get('http://localhost:8080/api/documents');\n        this.documents = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar documentos:', error);\n        alert('Erro ao carregar documentos');\n      }\n    },\n    showAddModal() {\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n      this.showModal = true;\n    },\n    editDocument(document) {\n      this.editingDocument = {\n        ...document\n      };\n      this.showModal = true;\n    },\n    async saveDocument() {\n      try {\n        if (this.editingDocument.id) {\n          await axios.put(`http://localhost:8080/api/documents/${this.editingDocument.id}`, this.editingDocument);\n        } else {\n          await axios.post('http://localhost:8080/api/documents', this.editingDocument);\n        }\n        this.closeModal();\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao salvar documento:', error);\n        alert('Erro ao salvar documento');\n      }\n    },\n    async deleteDocument(id) {\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\n        try {\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\n          this.loadDocuments();\n        } catch (error) {\n          console.error('Erro ao excluir documento:', error);\n          alert('Erro ao excluir documento');\n        }\n      }\n    },\n    async handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert('Erro ao fazer upload do arquivo');\n      }\n    },\n    closeModal() {\n      this.showModal = false;\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","documents","showModal","editingDocument","subject","content","locale","author","created","loadDocuments","methods","response","get","error","console","alert","showAddModal","editDocument","document","saveDocument","id","put","post","closeModal","deleteDocument","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\DocumentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"d-flex justify-content-between align-items-center mb-4\">\r\n      <h2>Gerenciamento de Documentos</h2>\r\n      <div>\r\n        <button class=\"btn btn-primary me-2\" @click=\"showAddModal\">\r\n          Novo Documento\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"d-none\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </div>\r\n\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Assunto</th>\r\n          <th>Conteúdo</th>\r\n          <th>Idioma</th>\r\n          <th>Autor</th>\r\n          <th>Tradutor</th>\r\n          <th>Ações</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"document in documents\" :key=\"document.id\">\r\n          <td>{{ document.subject }}</td>\r\n          <td>{{ document.content }}</td>\r\n          <td>{{ document.locale }}</td>\r\n          <td>{{ document.author }}</td>\r\n          <td>{{ document.translator?.name }}</td>\r\n          <td>\r\n            <button\r\n              class=\"btn btn-sm btn-primary me-2\"\r\n              @click=\"editDocument(document)\"\r\n            >\r\n              Editar\r\n            </button>\r\n            <button\r\n              class=\"btn btn-sm btn-danger\"\r\n              @click=\"deleteDocument(document.id)\"\r\n            >\r\n              Excluir\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Modal para Adicionar/Editar -->\r\n    <div class=\"modal\" v-if=\"showModal\" tabindex=\"-1\" style=\"display: block\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">\r\n              {{ editingDocument.id ? 'Editar' : 'Novo' }} Documento\r\n            </h5>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn-close\"\r\n              @click=\"closeModal\"\r\n            ></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <form @submit.prevent=\"saveDocument\">\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Assunto</label>\r\n                <input\r\n                  v-model=\"editingDocument.subject\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Conteúdo</label>\r\n                <textarea\r\n                  v-model=\"editingDocument.content\"\r\n                  class=\"form-control\"\r\n                  required\r\n                ></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Idioma</label>\r\n                <input\r\n                  v-model=\"editingDocument.locale\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Autor</label>\r\n                <input\r\n                  v-model=\"editingDocument.author\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"modal-footer\">\r\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                  Cancelar\r\n                </button>\r\n                <button type=\"submit\" class=\"btn btn-primary\">Salvar</button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'DocumentView',\r\n  data() {\r\n    return {\r\n      documents: [],\r\n      showModal: false,\r\n      editingDocument: {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.loadDocuments();\r\n  },\r\n  methods: {\r\n    async loadDocuments() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/documents');\r\n        this.documents = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar documentos:', error);\r\n        alert('Erro ao carregar documentos');\r\n      }\r\n    },\r\n    showAddModal() {\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n      this.showModal = true;\r\n    },\r\n    editDocument(document) {\r\n      this.editingDocument = { ...document };\r\n      this.showModal = true;\r\n    },\r\n    async saveDocument() {\r\n      try {\r\n        if (this.editingDocument.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/documents/${this.editingDocument.id}`,\r\n            this.editingDocument\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/documents',\r\n            this.editingDocument\r\n          );\r\n        }\r\n        this.closeModal();\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar documento:', error);\r\n        alert('Erro ao salvar documento');\r\n      }\r\n    },\r\n    async deleteDocument(id) {\r\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\r\n          this.loadDocuments();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir documento:', error);\r\n          alert('Erro ao excluir documento');\r\n        }\r\n      }\r\n    },\r\n    async handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert('Erro ao fazer upload do arquivo');\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>"],"mappings":"AA0HA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE;QACfC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAG,CAAC,qCAAqC,CAAC;QACvE,IAAI,CAACX,SAAQ,GAAIU,QAAQ,CAACX,IAAI;MAChC,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpDE,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACb,eAAc,GAAI;QACrBC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV,CAAC;MACD,IAAI,CAACL,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDe,YAAYA,CAACC,QAAQ,EAAE;MACrB,IAAI,CAACf,eAAc,GAAI;QAAE,GAAGe;MAAS,CAAC;MACtC,IAAI,CAAChB,SAAQ,GAAI,IAAI;IACvB,CAAC;IACD,MAAMiB,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,IAAI,IAAI,CAAChB,eAAe,CAACiB,EAAE,EAAE;UAC3B,MAAMtB,KAAK,CAACuB,GAAG,CACb,uCAAuC,IAAI,CAAClB,eAAe,CAACiB,EAAE,EAAE,EAChE,IAAI,CAACjB,eACP,CAAC;QACH,OAAO;UACL,MAAML,KAAK,CAACwB,IAAI,CACd,qCAAqC,EACrC,IAAI,CAACnB,eACP,CAAC;QACH;QACA,IAAI,CAACoB,UAAU,CAAC,CAAC;QACjB,IAAI,CAACd,aAAa,CAAC,CAAC;MACtB,EAAE,OAAOI,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjDE,KAAK,CAAC,0BAA0B,CAAC;MACnC;IACF,CAAC;IACD,MAAMS,cAAcA,CAACJ,EAAE,EAAE;MACvB,IAAIK,OAAO,CAAC,gDAAgD,CAAC,EAAE;QAC7D,IAAI;UACF,MAAM3B,KAAK,CAAC4B,MAAM,CAAC,uCAAuCN,EAAE,EAAE,CAAC;UAC/D,IAAI,CAACX,aAAa,CAAC,CAAC;QACtB,EAAE,OAAOI,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;UAClDE,KAAK,CAAC,2BAA2B,CAAC;QACpC;MACF;IACF,CAAC;IACD,MAAMY,gBAAgBA,CAACC,KAAK,EAAE;MAC5B,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,C