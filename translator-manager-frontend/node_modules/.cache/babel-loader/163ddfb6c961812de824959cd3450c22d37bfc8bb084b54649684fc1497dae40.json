{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'TranslatorView',\n  setup() {\n    const translators = ref([]);\n    const showModal = ref(false);\n    const editingTranslator = ref({\n      name: '',\n      email: '',\n      sourceLanguage: '',\n      targetLanguage: ''\n    });\n    const loadTranslators = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/api/translators');\n        translators.value = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar tradutores:', error);\n        alert('Erro ao carregar tradutores');\n      }\n    };\n    const showAddModal = () => {\n      editingTranslator.value = {\n        name: '',\n        email: '',\n        sourceLanguage: '',\n        targetLanguage: ''\n      };\n      showModal.value = true;\n    };\n    const editTranslator = translator => {\n      editingTranslator.value = {\n        ...translator\n      };\n      showModal.value = true;\n    };\n    const saveTranslator = async () => {\n      try {\n        if (editingTranslator.value.id) {\n          await axios.put(`http://localhost:8080/api/translators/${editingTranslator.value.id}`, editingTranslator.value);\n        } else {\n          await axios.post('http://localhost:8080/api/translators', editingTranslator.value);\n        }\n        await loadTranslators();\n        closeModal();\n      } catch (error) {\n        console.error('Erro ao salvar tradutor:', error);\n        alert('Erro ao salvar tradutor');\n      }\n    };\n    const confirmDelete = async translator => {\n      if (confirm('Tem certeza que deseja excluir este tradutor?')) {\n        try {\n          await axios.delete(`http://localhost:8080/api/translators/${translator.id}`);\n          await loadTranslators();\n        } catch (error) {\n          console.error('Erro ao excluir tradutor:', error);\n          alert('Erro ao excluir tradutor');\n        }\n      }\n    };\n    const handleFileUpload = async event => {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/translators/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        await loadTranslators();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert('Erro ao fazer upload do arquivo');\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      editingTranslator.value = {\n        name: '',\n        email: '',\n        sourceLanguage: '',\n        targetLanguage: ''\n      };\n    };\n    onMounted(loadTranslators);\n    return {\n      translators,\n      showModal,\n      editingTranslator,\n      showAddModal,\n      editTranslator,\n      saveTranslator,\n      confirmDelete,\n      handleFileUpload,\n      closeModal\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","name","setup","translators","showModal","editingTranslator","email","sourceLanguage","targetLanguage","loadTranslators","response","get","value","data","error","console","alert","showAddModal","editTranslator","translator","saveTranslator","id","put","post","closeModal","confirmDelete","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\TranslatorView.vue"],"sourcesContent":["<template>\r\n  <div class=\"translator-view\">\r\n    <header class=\"translator-view__header\">\r\n      <h1 class=\"translator-view__title\">Tradutores</h1>\r\n      <div class=\"translator-view__actions\">\r\n        <button class=\"btn btn-primary\" @click=\"showAddModal\">\r\n          <i class=\"fas fa-plus\"></i>\r\n          Novo Tradutor\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          <i class=\"fas fa-upload\"></i>\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"visually-hidden\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"translator-view__content\">\r\n      <div class=\"translator-grid\">\r\n        <div \r\n          v-for=\"translator in translators\" \r\n          :key=\"translator.id\"\r\n          class=\"translator-card\"\r\n        >\r\n          <div class=\"translator-card__header\">\r\n            <h3 class=\"translator-card__name\">{{ translator.name }}</h3>\r\n            <div class=\"translator-card__actions\">\r\n              <button\r\n                class=\"btn-icon\"\r\n                @click=\"editTranslator(translator)\"\r\n                title=\"Editar\"\r\n              >\r\n                <i class=\"fas fa-edit\"></i>\r\n              </button>\r\n              <button\r\n                class=\"btn-icon btn-icon--danger\"\r\n                @click=\"confirmDelete(translator)\"\r\n                title=\"Excluir\"\r\n              >\r\n                <i class=\"fas fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"translator-card__info\">\r\n            <p>\r\n              <i class=\"fas fa-envelope\"></i>\r\n              {{ translator.email }}\r\n            </p>\r\n            <p>\r\n              <i class=\"fas fa-language\"></i>\r\n              {{ translator.sourceLanguage }} → {{ translator.targetLanguage }}\r\n            </p>\r\n          </div>\r\n          \r\n          <div class=\"translator-card__stats\">\r\n            <div class=\"stat\">\r\n              <span class=\"stat__value\">{{ translator.documents?.length || 0 }}</span>\r\n              <span class=\"stat__label\">Documentos</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal de Tradutor -->\r\n    <div v-if=\"showModal\" class=\"modal\">\r\n      <div class=\"modal__content\">\r\n        <div class=\"modal__header\">\r\n          <h2 class=\"modal__title\">\r\n            {{ editingTranslator.id ? 'Editar' : 'Novo' }} Tradutor\r\n          </h2>\r\n          <button class=\"modal__close\" @click=\"closeModal\">×</button>\r\n        </div>\r\n        \r\n        <div class=\"modal__body\">\r\n          <form @submit.prevent=\"saveTranslator\" class=\"form\">\r\n            <div class=\"form__group\">\r\n              <label>Nome</label>\r\n              <input\r\n                v-model=\"editingTranslator.name\"\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form__group\">\r\n              <label>Email</label>\r\n              <input\r\n                v-model=\"editingTranslator.email\"\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form__row\">\r\n              <div class=\"form__group\">\r\n                <label>Idioma de Origem</label>\r\n                <input\r\n                  v-model=\"editingTranslator.sourceLanguage\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n\r\n              <div class=\"form__group\">\r\n                <label>Idioma de Destino</label>\r\n                <input\r\n                  v-model=\"editingTranslator.targetLanguage\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"modal__footer\">\r\n              <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                Cancelar\r\n              </button>\r\n              <button type=\"submit\" class=\"btn btn-primary\">\r\n                Salvar\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'TranslatorView',\r\n  \r\n  setup() {\r\n    const translators = ref([]);\r\n    const showModal = ref(false);\r\n    const editingTranslator = ref({\r\n      name: '',\r\n      email: '',\r\n      sourceLanguage: '',\r\n      targetLanguage: ''\r\n    });\r\n\r\n    const loadTranslators = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/translators');\r\n        translators.value = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar tradutores:', error);\r\n        alert('Erro ao carregar tradutores');\r\n      }\r\n    };\r\n\r\n    const showAddModal = () => {\r\n      editingTranslator.value = {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const editTranslator = (translator) => {\r\n      editingTranslator.value = { ...translator };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const saveTranslator = async () => {\r\n      try {\r\n        if (editingTranslator.value.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/translators/${editingTranslator.value.id}`,\r\n            editingTranslator.value\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/translators',\r\n            editingTranslator.value\r\n          );\r\n        }\r\n        await loadTranslators();\r\n        closeModal();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar tradutor:', error);\r\n        alert('Erro ao salvar tradutor');\r\n      }\r\n    };\r\n\r\n    const confirmDelete = async (translator) => {\r\n      if (confirm('Tem certeza que deseja excluir este tradutor?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/translators/${translator.id}`);\r\n          await loadTranslators();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir tradutor:', error);\r\n          alert('Erro ao excluir tradutor');\r\n        }\r\n      }\r\n    };\r\n\r\n    const handleFileUpload = async (event) => {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/translators/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        await loadTranslators();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert('Erro ao fazer upload do arquivo');\r\n      }\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n      editingTranslator.value = {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      };\r\n    };\r\n\r\n    onMounted(loadTranslators);\r\n\r\n    return {\r\n      translators,\r\n      showModal,\r\n      editingTranslator,\r\n      showAddModal,\r\n      editTranslator,\r\n      saveTranslator,\r\n      confirmDelete,\r\n      handleFileUpload,\r\n      closeModal\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.translator-view {\r\n  padding: var(--spacing-4);\r\n\r\n  &__header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-bottom: var(--spacing-4);\r\n\r\n    @media (max-width: 576px) {\r\n      flex-direction: column;\r\n      gap: var(--spacing-3);\r\n    }\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    font-size: 1.75rem;\r\n    color: var(--text-primary);\r\n  }\r\n\r\n  &__actions {\r\n    display: flex;\r\n    gap: var(--spacing-2);\r\n\r\n    @media (max-width: 576px) {\r\n      width: 100%;\r\n      \r\n      .btn {\r\n        flex: 1;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.translator-grid {\r\n  display: grid;\r\n  gap: var(--spacing-3);\r\n  grid-template-columns: repeat(1, 1fr);\r\n\r\n  @media (min-width: 576px) {\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n\r\n  @media (min-width: 992px) {\r\n    grid-template-columns: repeat(3, 1fr);\r\n  }\r\n\r\n  @media (min-width: 1200px) {\r\n    grid-template-columns: repeat(4, 1fr);\r\n  }\r\n}\r\n\r\n.translator-card {\r\n  background: white;\r\n  border-radius: var(--border-radius);\r\n  padding: var(--spacing-3);\r\n  box-shadow: var(--shadow-sm);\r\n  transition: transform 0.2s, box-shadow 0.2s;\r\n\r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    box-shadow: var(--shadow);\r\n  }\r\n\r\n  &__header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    margin-bottom: var(--spacing-3);\r\n  }\r\n\r\n  &__name {\r\n    margin: 0;\r\n    font-size: 1.25rem;\r\n    color: var(--text-primary);\r\n  }\r\n\r\n  &__info {\r\n    p {\r\n      margin: var(--spacing-2) 0;\r\n      color: var(--text-secondary);\r\n      display: flex;\r\n      align-items: center;\r\n      gap: var(--spacing-2);\r\n\r\n      i {\r\n        color: var(--primary-color);\r\n        width: 16px;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__stats {\r\n    margin-top: var(--spacing-3);\r\n    padding-top: var(--spacing-3);\r\n    border-top: 1px solid var(--gray-200);\r\n    text-align: center;\r\n\r\n    .stat__value {\r\n      font-size: 1.5rem;\r\n      font-weight: bold;\r\n      color: var(--primary-color);\r\n      display: block;\r\n    }\r\n\r\n    .stat__label {\r\n      color: var(--text-secondary);\r\n      font-size: 0.875rem;\r\n    }\r\n  }\r\n}\r\n\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  padding: var(--spacing-2);\r\n  cursor: pointer;\r\n  color: var(--gray-600);\r\n  border-radius: var(--border-radius);\r\n  transition: all 0.2s;\r\n\r\n  &:hover {\r\n    background-color: var(--gray-100);\r\n    color: var(--primary-color);\r\n  }\r\n\r\n  &--danger:hover {\r\n    background-color: var(--danger-color);\r\n    color: white;\r\n  }\r\n}\r\n\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1000;\r\n\r\n  &__content {\r\n    background: white;\r\n    border-radius: var(--border-radius);\r\n    width: 90%;\r\n    max-width: 500px;\r\n    max-height: 90vh;\r\n    overflow-y: auto;\r\n  }\r\n\r\n  &__header {\r\n    padding: var(--spacing-3);\r\n    border-bottom: 1px solid var(--gray-200);\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    font-size: 1.25rem;\r\n  }\r\n\r\n  &__close {\r\n    background: none;\r\n    border: none;\r\n    font-size: 1.5rem;\r\n    cursor: pointer;\r\n    color: var(--gray-600);\r\n    \r\n    &:hover {\r\n      color: var(--gray-900);\r\n    }\r\n  }\r\n\r\n  &__body {\r\n    padding: var(--spacing-3);\r\n  }\r\n\r\n  &__footer {\r\n    padding: var(--spacing-3);\r\n    border-top: 1px solid var(--gray-200);\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    gap: var(--spacing-2);\r\n  }\r\n}\r\n\r\n.form {\r\n  &__group {\r\n    margin-bottom: var(--spacing-3);\r\n\r\n    label {\r\n      display: block;\r\n      margin-bottom: var(--spacing-1);\r\n      color: var(--text-primary);\r\n      font-weight: 500;\r\n    }\r\n  }\r\n\r\n  &__row {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n    gap: var(--spacing-3);\r\n\r\n    @media (max-width: 576px) {\r\n      grid-template-columns: 1fr;\r\n    }\r\n  }\r\n}\r\n\r\n.visually-hidden {\r\n  position: absolute;\r\n  width: 1px;\r\n  height: 1px;\r\n  padding: 0;\r\n  margin: -1px;\r\n  overflow: hidden;\r\n  clip: rect(0, 0, 0, 0);\r\n  white-space: nowrap;\r\n  border: 0;\r\n}\r\n</style>"],"mappings":"AA4IA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EAEtBC,KAAKA,CAAA,EAAG;IACN,MAAMC,WAAU,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMM,SAAQ,GAAIN,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMO,iBAAgB,GAAIP,GAAG,CAAC;MAC5BG,IAAI,EAAE,EAAE;MACRK,KAAK,EAAE,EAAE;MACTC,cAAc,EAAE,EAAE;MAClBC,cAAc,EAAE;IAClB,CAAC,CAAC;IAEF,MAAMC,eAAc,GAAI,MAAAA,CAAA,KAAY;MAClC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,uCAAuC,CAAC;QACzER,WAAW,CAACS,KAAI,GAAIF,QAAQ,CAACG,IAAI;MACnC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpDE,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF,CAAC;IAED,MAAMC,YAAW,GAAIA,CAAA,KAAM;MACzBZ,iBAAiB,CAACO,KAAI,GAAI;QACxBX,IAAI,EAAE,EAAE;QACRK,KAAK,EAAE,EAAE;QACTC,cAAc,EAAE,EAAE;QAClBC,cAAc,EAAE;MAClB,CAAC;MACDJ,SAAS,CAACQ,KAAI,GAAI,IAAI;IACxB,CAAC;IAED,MAAMM,cAAa,GAAKC,UAAU,IAAK;MACrCd,iBAAiB,CAACO,KAAI,GAAI;QAAE,GAAGO;MAAW,CAAC;MAC3Cf,SAAS,CAACQ,KAAI,GAAI,IAAI;IACxB,CAAC;IAED,MAAMQ,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjC,IAAI;QACF,IAAIf,iBAAiB,CAACO,KAAK,CAACS,EAAE,EAAE;UAC9B,MAAMrB,KAAK,CAACsB,GAAG,CACb,yCAAyCjB,iBAAiB,CAACO,KAAK,CAACS,EAAE,EAAE,EACrEhB,iBAAiB,CAACO,KACpB,CAAC;QACH,OAAO;UACL,MAAMZ,KAAK,CAACuB,IAAI,CACd,uCAAuC,EACvClB,iBAAiB,CAACO,KACpB,CAAC;QACH;QACA,MAAMH,eAAe,CAAC,CAAC;QACvBe,UAAU,CAAC,CAAC;MACd,EAAE,OAAOV,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;QAChDE,KAAK,CAAC,yBAAyB,CAAC;MAClC;IACF,CAAC;IAED,MAAMS,aAAY,GAAI,MAAON,UAAU,IAAK;MAC1C,IAAIO,OAAO,CAAC,+CAA+C,CAAC,EAAE;QAC5D,IAAI;UACF,MAAM1B,