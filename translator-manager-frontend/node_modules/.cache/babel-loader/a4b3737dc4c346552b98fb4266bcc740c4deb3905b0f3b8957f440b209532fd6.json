sted_24","onSubmit","_withModifiers","saveDocument","_hoisted_25","required","_hoisted_26","rows","_hoisted_27","_hoisted_28","_hoisted_29","_hoisted_30"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\DocumentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"document-view\">\r\n    <header class=\"document-view__header\">\r\n      <h1 class=\"document-view__title\">Documentos</h1>\r\n      <div class=\"document-view__actions\">\r\n        <button class=\"btn btn-primary\" @click=\"showAddModal\">\r\n          <i class=\"fas fa-plus\"></i>\r\n          Novo Documento\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          <i class=\"fas fa-upload\"></i>\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"visually-hidden\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"document-view__filters\">\r\n      <div class=\"search-field\">\r\n        <i class=\"fas fa-search search-field__icon\"></i>\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchTerm\"\r\n          placeholder=\"Pesquisar documentos...\"\r\n          class=\"form-control\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"document-view__content\">\r\n      <div class=\"document-grid\">\r\n        <div\r\n          v-for=\"document in documents\"\r\n          :key=\"document.id\"\r\n          class=\"document-card\"\r\n        >\r\n          <div class=\"document-card__header\">\r\n            <div class=\"document-card__title-group\">\r\n              <h3 class=\"document-card__title\">{{ document.subject }}</h3>\r\n              <span class=\"document-card__locale\">\r\n                <i class=\"fas fa-globe\"></i>\r\n                {{ document.locale }}\r\n              </span>\r\n            </div>\r\n            <div class=\"document-card__actions\">\r\n              <button\r\n                class=\"btn-icon\"\r\n                @click=\"editDocument(document)\"\r\n                title=\"Editar\"\r\n              >\r\n                <i class=\"fas fa-edit\"></i>\r\n              </button>\r\n              <button\r\n                class=\"btn-icon btn-icon--danger\"\r\n                @click=\"confirmDelete(document)\"\r\n                title=\"Excluir\"\r\n              >\r\n                <i class=\"fas fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"document-card__content\">\r\n            <p class=\"document-card__text\">{{ truncateText(document.content) }}</p>\r\n          </div>\r\n\r\n          <div class=\"document-card__footer\">\r\n            <div class=\"document-card__info\">\r\n              <p>\r\n                <i class=\"fas fa-user\"></i>\r\n                {{ document.author }}\r\n              </p>\r\n              <p>\r\n                <i class=\"fas fa-user-edit\"></i>\r\n                {{ document.translator?.name || 'Sem tradutor' }}\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal de Documento -->\r\n    <div v-if=\"showModal\" class=\"modal\">\r\n      <div class=\"modal__content\">\r\n        <div class=\"modal__header\">\r\n          <h2 class=\"modal__title\">\r\n            {{ editingDocument.id ? 'Editar' : 'Novo' }} Documento\r\n          </h2>\r\n          <button class=\"modal__close\" @click=\"closeModal\">×</button>\r\n        </div>\r\n        \r\n        <div class=\"modal__body\">\r\n          <form @submit.prevent=\"saveDocument\" class=\"form\">\r\n            <div class=\"form__group\">\r\n              <label>Assunto</label>\r\n              <input\r\n                v-model=\"editingDocument.subject\"\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form__group\">\r\n              <label>Conteúdo</label>\r\n              <textarea\r\n                v-model=\"editingDocument.content\"\r\n                class=\"form-control\"\r\n                rows=\"5\"\r\n                required\r\n              ></textarea>\r\n            </div>\r\n\r\n            <div class=\"form__row\">\r\n              <div class=\"form__group\">\r\n                <label>Idioma</label>\r\n                <input\r\n                  v-model=\"editingDocument.locale\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n\r\n              <div class=\"form__group\">\r\n                <label>Autor</label>\r\n                <input\r\n                  v-model=\"editingDocument.author\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"modal__footer\">\r\n              <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                Cancelar\r\n              </button>\r\n              <button type=\"submit\" class=\"btn btn-primary\">\r\n                Salvar\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'DocumentView',\r\n  \r\n  setup() {\r\n    const documents = ref([]);\r\n    const showModal = ref(false);\r\n    const searchTerm = ref('');\r\n    const editingDocument = ref({\r\n      subject: '',\r\n      content: '',\r\n      locale: '',\r\n      author: ''\r\n    });\r\n\r\n    const loadDocuments = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/documents');\r\n        documents.value = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar documentos:', error);\r\n        alert('Erro ao carregar documentos');\r\n      }\r\n    };\r\n\r\n    const showAddModal = () => {\r\n      editingDocument.value = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const editDocument = (document) => {\r\n      editingDocument.value = { ...document };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const saveDocument = async () => {\r\n      try {\r\n        if (editingDocument.value.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/documents/${editingDocument.value.id}`,\r\n            editingDocument.value\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/documents',\r\n            editingDocument.value\r\n          );\r\n        }\r\n        await loadDocuments();\r\n        closeModal();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar documento:', error);\r\n        alert('Erro ao salvar documento');\r\n      }\r\n    };\r\n\r\n    const confirmDelete = async (document) => {\r\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/documents/${document.id}`);\r\n          await loadDocuments();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir documento:', error);\r\n          alert('Erro ao excluir documento');\r\n        }\r\n      }\r\n    };\r\n\r\n    const handleFileUpload = async (event) => {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http:/