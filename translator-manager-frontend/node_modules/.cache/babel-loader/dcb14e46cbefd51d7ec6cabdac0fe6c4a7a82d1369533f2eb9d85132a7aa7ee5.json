{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'DocumentView',\n  setup() {\n    const documents = ref([]);\n    const showModal = ref(false);\n    const searchTerm = ref('');\n    const editingDocument = ref({\n      subject: '',\n      content: '',\n      locale: '',\n      author: ''\n    });\n    const loadDocuments = async () => {\n      try {\n        const response = await axios.get('http://localhost:8080/api/documents');\n        documents.value = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar documentos:', error);\n        alert('Erro ao carregar documentos');\n      }\n    };\n    const showAddModal = () => {\n      editingDocument.value = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n      showModal.value = true;\n    };\n    const editDocument = document => {\n      editingDocument.value = {\n        ...document\n      };\n      showModal.value = true;\n    };\n    const saveDocument = async () => {\n      try {\n        if (editingDocument.value.id) {\n          await axios.put(`http://localhost:8080/api/documents/${editingDocument.value.id}`, editingDocument.value);\n        } else {\n          await axios.post('http://localhost:8080/api/documents', editingDocument.value);\n        }\n        await loadDocuments();\n        closeModal();\n      } catch (error) {\n        console.error('Erro ao salvar documento:', error);\n        alert('Erro ao salvar documento');\n      }\n    };\n    const confirmDelete = async document => {\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\n        try {\n          await axios.delete(`http://localhost:8080/api/documents/${document.id}`);\n          await loadDocuments();\n        } catch (error) {\n          console.error('Erro ao excluir documento:', error);\n          alert('Erro ao excluir documento');\n        }\n      }\n    };\n    const handleFileUpload = async event => {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        await loadDocuments();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert('Erro ao fazer upload do arquivo');\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      editingDocument.value = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n    };\n    const truncateText = (text, length = 150) => {\n      if (!text) return '';\n      return text.length <= length ? text : `${text.substring(0, length)}...`;\n    };\n    onMounted(loadDocuments);\n    return {\n      documents,\n      showModal,\n      searchTerm,\n      editingDocument,\n      showAddModal,\n      editDocument,\n      saveDocument,\n      confirmDelete,\n      handleFileUpload,\n      closeModal,\n      truncateText\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","name","setup","documents","showModal","searchTerm","editingDocument","subject","content","locale","author","loadDocuments","response","get","value","data","error","console","alert","showAddModal","editDocument","document","saveDocument","id","put","post","closeModal","confirmDelete","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers","truncateText","text","length","substring"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\DocumentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"document-view\">\r\n    <header class=\"document-view__header\">\r\n      <h1 class=\"document-view__title\">Documentos</h1>\r\n      <div class=\"document-view__actions\">\r\n        <button class=\"btn btn-primary\" @click=\"showAddModal\">\r\n          <i class=\"fas fa-plus\"></i>\r\n          Novo Documento\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          <i class=\"fas fa-upload\"></i>\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"visually-hidden\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"document-view__filters\">\r\n      <div class=\"search-field\">\r\n        <i class=\"fas fa-search search-field__icon\"></i>\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchTerm\"\r\n          placeholder=\"Pesquisar documentos...\"\r\n          class=\"form-control\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"document-view__content\">\r\n      <div class=\"document-grid\">\r\n        <div\r\n          v-for=\"document in documents\"\r\n          :key=\"document.id\"\r\n          class=\"document-card\"\r\n        >\r\n          <div class=\"document-card__header\">\r\n            <div class=\"document-card__title-group\">\r\n              <h3 class=\"document-card__title\">{{ document.subject }}</h3>\r\n              <span class=\"document-card__locale\">\r\n                <i class=\"fas fa-globe\"></i>\r\n                {{ document.locale }}\r\n              </span>\r\n            </div>\r\n            <div class=\"document-card__actions\">\r\n              <button\r\n                class=\"btn-icon\"\r\n                @click=\"editDocument(document)\"\r\n                title=\"Editar\"\r\n              >\r\n                <i class=\"fas fa-edit\"></i>\r\n              </button>\r\n              <button\r\n                class=\"btn-icon btn-icon--danger\"\r\n                @click=\"confirmDelete(document)\"\r\n                title=\"Excluir\"\r\n              >\r\n                <i class=\"fas fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"document-card__content\">\r\n            <p class=\"document-card__text\">{{ truncateText(document.content) }}</p>\r\n          </div>\r\n\r\n          <div class=\"document-card__footer\">\r\n            <div class=\"document-card__info\">\r\n              <p>\r\n                <i class=\"fas fa-user\"></i>\r\n                {{ document.author }}\r\n              </p>\r\n              <p>\r\n                <i class=\"fas fa-user-edit\"></i>\r\n                {{ document.translator?.name || 'Sem tradutor' }}\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal de Documento -->\r\n    <div v-if=\"showModal\" class=\"modal\">\r\n      <div class=\"modal__content\">\r\n        <div class=\"modal__header\">\r\n          <h2 class=\"modal__title\">\r\n            {{ editingDocument.id ? 'Editar' : 'Novo' }} Documento\r\n          </h2>\r\n          <button class=\"modal__close\" @click=\"closeModal\">×</button>\r\n        </div>\r\n        \r\n        <div class=\"modal__body\">\r\n          <form @submit.prevent=\"saveDocument\" class=\"form\">\r\n            <div class=\"form__group\">\r\n              <label>Assunto</label>\r\n              <input\r\n                v-model=\"editingDocument.subject\"\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form__group\">\r\n              <label>Conteúdo</label>\r\n              <textarea\r\n                v-model=\"editingDocument.content\"\r\n                class=\"form-control\"\r\n                rows=\"5\"\r\n                required\r\n              ></textarea>\r\n            </div>\r\n\r\n            <div class=\"form__row\">\r\n              <div class=\"form__group\">\r\n                <label>Idioma</label>\r\n                <input\r\n                  v-model=\"editingDocument.locale\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n\r\n              <div class=\"form__group\">\r\n                <label>Autor</label>\r\n                <input\r\n                  v-model=\"editingDocument.author\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"modal__footer\">\r\n              <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                Cancelar\r\n              </button>\r\n              <button type=\"submit\" class=\"btn btn-primary\">\r\n                Salvar\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'DocumentView',\r\n  \r\n  setup() {\r\n    const documents = ref([]);\r\n    const showModal = ref(false);\r\n    const searchTerm = ref('');\r\n    const editingDocument = ref({\r\n      subject: '',\r\n      content: '',\r\n      locale: '',\r\n      author: ''\r\n    });\r\n\r\n    const loadDocuments = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/documents');\r\n        documents.value = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar documentos:', error);\r\n        alert('Erro ao carregar documentos');\r\n      }\r\n    };\r\n\r\n    const showAddModal = () => {\r\n      editingDocument.value = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const editDocument = (document) => {\r\n      editingDocument.value = { ...document };\r\n      showModal.value = true;\r\n    };\r\n\r\n    const saveDocument = async () => {\r\n      try {\r\n        if (editingDocument.value.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/documents/${editingDocument.value.id}`,\r\n            editingDocument.value\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/documents',\r\n            editingDocument.value\r\n          );\r\n        }\r\n        await loadDocuments();\r\n        closeModal();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar documento:', error);\r\n        alert('Erro ao salvar documento');\r\n      }\r\n    };\r\n\r\n    const confirmDelete = async (document) => {\r\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/documents/${document.id}`);\r\n          await loadDocuments();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir documento:', error);\r\n          alert('Erro ao excluir documento');\r\n        }\r\n      }\r\n    };\r\n\r\n    const handleFileUpload = async (event) => {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        await loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert('Erro ao fazer upload do arquivo');\r\n      }\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n      editingDocument.value = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n    };\r\n\r\n    const truncateText = (text, length = 150) => {\r\n      if (!text) return '';\r\n      return text.length <= length ? text : `${text.substring(0, length)}...`;\r\n    };\r\n\r\n    onMounted(loadDocuments);\r\n\r\n    return {\r\n      documents,\r\n      showModal,\r\n      searchTerm,\r\n      editingDocument,\r\n      showAddModal,\r\n      editDocument,\r\n      saveDocument,\r\n      confirmDelete,\r\n      handleFileUpload,\r\n      closeModal,\r\n      truncateText\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.document-view {\r\n  padding: var(--spacing-4);\r\n\r\n  &__header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-bottom: var(--spacing-4);\r\n\r\n    @media (max-width: 576px) {\r\n      flex-direction: column;\r\n      gap: var(--spacing-3);\r\n    }\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    font-size: 1.75rem;\r\n    color: var(--text-primary);\r\n  }\r\n\r\n  &__actions {\r\n    display: flex;\r\n    gap: var(--spacing-2);\r\n\r\n    @media (max-width: 576px) {\r\n      width: 100%;\r\n      \r\n      .btn {\r\n        flex: 1;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__filters {\r\n    margin-bottom: var(--spacing-4);\r\n  }\r\n}\r\n\r\n.search-field {\r\n  position: relative;\r\n\r\n  &__icon {\r\n    position: absolute;\r\n    left: var(--spacing-3);\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    color: var(--gray-500);\r\n  }\r\n\r\n  .form-control {\r\n    padding-left: calc(var(--spacing-3) * 2 + 16px);\r\n  }\r\n}\r\n\r\n.document-grid {\r\n  display: grid;\r\n  gap: var(--spacing-3);\r\n  grid-template-columns: repeat(1, 1fr);\r\n\r\n  @media (min-width: 576px) {\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n\r\n  @media (min-width: 992px) {\r\n    grid-template-columns: repeat(3, 1fr);\r\n  }\r\n\r\n  @media (min-width: 1200px) {\r\n    grid-template-columns: repeat(4, 1fr);\r\n  }\r\n}\r\n\r\n.document-card {\r\n  background: white;\r\n  border-radius: var(--border-radius);\r\n  padding: var(--spacing-3);\r\n  box-shadow: var(--shadow-sm);\r\n  transition: transform 0.2s, box-shadow 0.2s;\r\n\r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    box-shadow: var(--shadow);\r\n  }\r\n\r\n  &__header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    margin-bottom: var(--spacing-3);\r\n  }\r\n\r\n  &__title-group {\r\n    flex: 1;\r\n    min-width: 0;\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    font-size: 1.25rem;\r\n    color: var(--text-primary);\r\n    margin-bottom: var(--spacing-2);\r\n  }\r\n\r\n  &__locale {\r\n    display: inline-flex;\r\n    align-items: center;\r\n    gap: var(--spacing-1);\r\n    font-size: 0.875rem;\r\n    color: var(--text-secondary);\r\n    background-color: var(--gray-100);\r\n    padding: 2px var(--spacing-2);\r\n    border-radius: var(--border-radius-sm);\r\n  }\r\n\r\n  &__content {\r\n    margin-bottom: var(--spacing-3);\r\n  }\r\n\r\n  &__text {\r\n    color: var(--text-secondary);\r\n    font-size: 0.875rem;\r\n    line-height: 1.5;\r\n    margin: 0;\r\n  }\r\n\r\n  &__footer {\r\n    padding-top: var(--spacing-3);\r\n    border-top: 1px solid var(--gray-200);\r\n  }\r\n\r\n  &__info {\r\n    p {\r\n      margin: var(--spacing-1) 0;\r\n      color: var(--text-secondary);\r\n      font-size: 0.875rem;\r\n      display: flex;\r\n      align-items: center;\r\n      gap: var(--spacing-2);\r\n\r\n      i {\r\n        color: var(--primary-color);\r\n        width: 16px;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  padding: var(--spacing-2);\r\n  cursor: pointer;\r\n  color: var(--gray-600);\r\n  border-radius: var(--border-radius);\r\n  transition: all 0.2s;\r\n\r\n  &:hover {\r\n  