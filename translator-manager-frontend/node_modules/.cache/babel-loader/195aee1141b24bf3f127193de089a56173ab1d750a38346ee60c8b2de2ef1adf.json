{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'DocumentView',\n  data() {\n    return {\n      documents: {\n        content: [],\n        number: 0,\n        size: 10,\n        totalPages: 0,\n        first: true,\n        last: true\n      },\n      showModal: false,\n      editingDocument: {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      }\n    };\n  },\n  created() {\n    this.loadDocuments();\n  },\n  methods: {\n    async loadDocuments(page = 0) {\n      try {\n        const response = await axios.get(`http://localhost:8080/api/documents?page=${page}&size=${this.documents.size}`);\n        this.documents = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar documentos:', error);\n        alert(this.$t('errors.loadError'));\n      }\n    },\n    changePage(page) {\n      this.loadDocuments(page);\n    },\n    showAddModal() {\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n      this.showModal = true;\n    },\n    editDocument(document) {\n      this.editingDocument = {\n        ...document\n      };\n      this.showModal = true;\n    },\n    async saveDocument() {\n      try {\n        if (this.editingDocument.id) {\n          await axios.put(`http://localhost:8080/api/documents/${this.editingDocument.id}`, this.editingDocument);\n        } else {\n          await axios.post('http://localhost:8080/api/documents', this.editingDocument);\n        }\n        this.closeModal();\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao salvar documento:', error);\n        alert(this.$t('errors.saveError'));\n      }\n    },\n    async deleteDocument(id) {\n      if (confirm(this.$t('common.confirmDelete'))) {\n        try {\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\n          this.loadDocuments();\n        } catch (error) {\n          console.error('Erro ao excluir documento:', error);\n          alert(this.$t('errors.deleteError'));\n        }\n      }\n    },\n    async handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert(this.$t('errors.uploadError'));\n      }\n    },\n    downloadCSVTemplate() {\n      const template = 'subject,content,locale,author\\n\"Exemplo de Assunto\",\"Exemplo de Conteúdo\",\"pt-BR\",\"Autor Exemplo\"';\n      const blob = new Blob([template], {\n        type: 'text/csv'\n      });\n      const url = window.URL.createObjectURL(blob);\n      const a = document.createElement('a');\n      a.href = url;\n      a.download = 'document_template.csv';\n      document.body.appendChild(a);\n      a.click();\n      window.URL.revokeObjectURL(url);\n      document.body.removeChild(a);\n    },\n    closeModal() {\n      this.showModal = false;\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","documents","content","number","size","totalPages","first","last","showModal","editingDocument","subject","locale","author","created","loadDocuments","methods","page","response","get","error","console","alert","$t","changePage","showAddModal","editDocument","document","saveDocument","id","put","post","closeModal","deleteDocument","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers","downloadCSVTemplate","template","blob","Blob","type","url","window","URL","createObjectURL","a","createElement","href","download","body","appendChild","click","revokeObjectURL","removeChild"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\DocumentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"d-flex justify-content-between align-items-center mb-4\">\r\n      <h2>{{ $t('document.title') }}</h2>\r\n      <div class=\"d-flex align-items-center\">\r\n        <div class=\"me-3\">\r\n          <label class=\"form-label me-2\">{{ $t('common.itemsPerPage') }}</label>\r\n          <select class=\"form-select form-select-sm d-inline-block w-auto\" v-model=\"documents.size\" @change=\"loadDocuments(0)\">\r\n            <option value=\"5\">5</option>\r\n            <option value=\"10\">10</option>\r\n            <option value=\"20\">20</option>\r\n            <option value=\"50\">50</option>\r\n          </select>\r\n        </div>\r\n        <button class=\"btn btn-primary me-2\" @click=\"showAddModal\">\r\n          {{ $t('document.newDocument') }}\r\n        </button>\r\n        <div class=\"btn-group\">\r\n    \r\n          <label class=\"btn btn-secondary\">\r\n            <i class=\"bi bi-upload me-1\"></i>\r\n            {{ $t('common.uploadCSV') }}\r\n            <input\r\n              type=\"file\"\r\n              accept=\".csv\"\r\n              class=\"d-none\"\r\n              @change=\"handleFileUpload\"\r\n            />\r\n          </label>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <table class=\"table table-sm table-hover\">\r\n      <thead class=\"table-light\">\r\n        <tr>\r\n          <th class=\"text-center\" style=\"width: 50px\">#</th>\r\n          <th>{{ $t('document.subject') }}</th>\r\n          <th>{{ $t('document.locale') }}</th>\r\n          <th>{{ $t('document.author') }}</th>\r\n          <th>{{ $t('document.translator') }}</th>\r\n          <th class=\"text-center\" style=\"width: 100px\">{{ $t('common.actions') }}</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(document, index) in documents.content\" :key=\"document.id\">\r\n          <td class=\"text-center\">{{ (documents.number * documents.size) + index + 1 }}</td>\r\n          <td>{{ document.subject }}</td>\r\n          <td>{{ document.locale }}</td>\r\n          <td>{{ document.author }}</td>\r\n          <td>{{ document.translator?.name }}</td>\r\n          <td>\r\n            <div class=\"d-flex justify-content-center\">\r\n              <button\r\n                class=\"btn btn-sm btn-primary me-2\"\r\n                @click=\"editDocument(document)\"\r\n                :title=\"$t('common.edit')\"\r\n              >\r\n                <i class=\"bi bi-pencil\"></i>\r\n              </button>\r\n              <button\r\n                class=\"btn btn-sm btn-danger\"\r\n                @click=\"deleteDocument(document.id)\"\r\n                :title=\"$t('common.delete')\"\r\n              >\r\n                <i class=\"bi bi-x-lg\"></i>\r\n              </button>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Paginação -->\r\n    <nav aria-label=\"Navegação de páginas\" class=\"mt-4\">\r\n      <ul class=\"pagination justify-content-center\">\r\n        <li class=\"page-item\" :class=\"{ disabled: documents.first }\">\r\n          <a class=\"page-link\" href=\"#\" @click.prevent=\"changePage(documents.number - 1)\">{{ $t('common.previous') }}</a>\r\n        </li>\r\n        <li class=\"page-item\" v-for=\"page in totalPages\" :key=\"page\" :class=\"{ active: page - 1 === documents.number }\">\r\n          <a class=\"page-link\" href=\"#\" @click.prevent=\"changePage(page - 1)\">{{ page }}</a>\r\n        </li>\r\n        <li class=\"page-item\" :class=\"{ disabled: documents.last }\">\r\n          <a class=\"page-link\" href=\"#\" @click.prevent=\"changePage(documents.number + 1)\">{{ $t('common.next') }}</a>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n\r\n    <!-- Modal para Adicionar/Editar -->\r\n    <div class=\"modal\" v-if=\"showModal\" tabindex=\"-1\" style=\"display: block\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">\r\n              {{ editingDocument.id ? $t('document.editDocument') : $t('document.newDocument') }}\r\n            </h5>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn-close\"\r\n              @click=\"closeModal\"\r\n            ></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <form @submit.prevent=\"saveDocument\">\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">{{ $t('document.subject') }}</label>\r\n                <input\r\n                  v-model=\"editingDocument.subject\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">{{ $t('document.content') }}</label>\r\n                <textarea\r\n                  v-model=\"editingDocument.content\"\r\n                  class=\"form-control\"\r\n                  required\r\n                ></textarea>\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">{{ $t('document.locale') }}</label>\r\n                <input\r\n                  v-model=\"editingDocument.locale\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">{{ $t('document.author') }}</label>\r\n                <input\r\n                  v-model=\"editingDocument.author\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"modal-footer\">\r\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                  {{ $t('common.cancel') }}\r\n                </button>\r\n                <button type=\"submit\" class=\"btn btn-primary\">{{ $t('common.save') }}</button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'DocumentView',\r\n  data() {\r\n    return {\r\n      documents: {\r\n        content: [],\r\n        number: 0,\r\n        size: 10,\r\n        totalPages: 0,\r\n        first: true,\r\n        last: true\r\n      },\r\n      showModal: false,\r\n      editingDocument: {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.loadDocuments();\r\n  },\r\n  methods: {\r\n    async loadDocuments(page = 0) {\r\n      try {\r\n        const response = await axios.get(`http://localhost:8080/api/documents?page=${page}&size=${this.documents.size}`);\r\n        this.documents = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar documentos:', error);\r\n        alert(this.$t('errors.loadError'));\r\n      }\r\n    },\r\n    changePage(page) {\r\n      this.loadDocuments(page);\r\n    },\r\n    showAddModal() {\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n      this.showModal = true;\r\n    },\r\n    editDocument(document) {\r\n      this.editingDocument = { ...document };\r\n      this.showModal = true;\r\n    },\r\n    async saveDocument() {\r\n      try {\r\n        if (this.editingDocument.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/documents/${this.editingDocument.id}`,\r\n            this.editingDocument\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/documents',\r\n            this.editingDocument\r\n          );\r\n        }\r\n        this.closeModal();\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar documento:', error);\r\n        alert(this.$t('errors.saveError'));\r\n      }\r\n    },\r\n    async deleteDocument(id) {\r\n      if (confirm(this.$t('common.confirmDelete'))) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\r\n          this.loadDocuments();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir documento:', error);\r\n          alert(this.$t('errors.deleteError'));\r\n        }\r\n      }\r\n    },\r\n    async handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert(this.$t('errors.uploadError'));\r\n      }\r\n    },\r\n    downloadCSVTemplate() {\r\n      const template = 'subject,content,locale,author\\n\"Exemplo de Assunto\",\"Exemplo de Conteúdo\",\"pt-BR\",\"Autor Exemplo\"';\r\n      const blob = new Blob([template], { type: 'text/csv' });\r\n      const url = window.URL.createObjectURL(blob);\r\n      const a = document.createElement('a');\r\n      a.href = url;\r\n      a.download = 'document_template.csv';\r\n      document.body.appendChild(a);\r\n      a.click();\r\n      window.URL.revokeObjectURL(url);\r\n      document.body.removeChild(a);\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>"],"mappings":"AA2JA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,CAAC;QACTC,IAAI,EAAE,EAAE;QACRC,UAAU,EAAE,CAAC;QACbC,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE;MACR,CAAC;MACDC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE;QACfC,OAAO,EAAE,EAAE;QACXR,OAAO,EAAE,EAAE;QACXS,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAACE,IAAG,GAAI,CAAC,EAAE;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMnB,KAAK,CAACoB,GAAG,CAAC,4CAA4CF,IAAI,SAAS,IAAI,CAACf,SAAS,CAACG,IAAI,EAAE,CAAC;QAChH,IAAI,CAACH,SAAQ,GAAIgB,QAAQ,CAACjB,IAAI;MAChC,EAAE,OAAOmB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpDE,KAAK,CAAC,IAAI,CAACC,EAAE,CAAC,kBAAkB,CAAC,CAAC;MACpC;IACF,CAAC;IACDC,UAAUA,CAACP,IAAI,EAAE;MACf,IAAI,CAACF,aAAa,CAACE,IAAI,CAAC;IAC1B,CAAC;IACDQ,YAAYA,CAAA,EAAG;MACb,IAAI,CAACf,eAAc,GAAI;QACrBC,OAAO,EAAE,EAAE;QACXR,OAAO,EAAE,EAAE;QACXS,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV,CAAC;MACD,IAAI,CAACJ,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDiB,YAAYA,CAACC,QAAQ,EAAE;MACrB,IAAI,CAACjB,eAAc,GAAI;QAAE,GAAGiB;MAAS,CAAC;MACtC,IAAI,CAAClB,SAAQ,GAAI,IAAI;IACvB,CAAC;IACD,MAAMmB,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,IAAI,IAAI,CAAClB,eAAe,CAACmB,EAAE,EAAE;UAC3B,MAAM9B,KAAK,CAAC+B,GAAG,CACb,uCAAuC,IAAI,CAACpB,eAAe,CAACmB,EAAE,EAAE,EAChE,IAAI,CAACnB,eACP,CAAC;QACH,OAAO;UACL,MAAMX,KAAK,CAACgC,IAAI,CACd,qCAAqC,EACrC,IAAI,CAACrB,eACP,CAAC;QACH;QACA,IAAI,CAACsB,UAAU,CAAC,CAAC;QACjB,IAAI,CAACjB,aAAa,CAAC,CAAC;MACtB,EAAE,OAAOK,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjDE,KAA