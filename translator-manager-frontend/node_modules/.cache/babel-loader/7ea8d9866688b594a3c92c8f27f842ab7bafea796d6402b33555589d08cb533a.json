{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'DocumentView',\n  data() {\n    return {\n      documents: [],\n      showModal: false,\n      editingDocument: {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      }\n    };\n  },\n  created() {\n    this.loadDocuments();\n  },\n  methods: {\n    async loadDocuments() {\n      try {\n        const response = await axios.get('http://localhost:8080/api/documents');\n        this.documents = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar documentos:', error);\n        alert('Erro ao carregar documentos');\n      }\n    },\n    showAddModal() {\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n      this.showModal = true;\n    },\n    editDocument(document) {\n      this.editingDocument = {\n        ...document\n      };\n      this.showModal = true;\n    },\n    async saveDocument() {\n      try {\n        if (this.editingDocument.id) {\n          await axios.put(`http://localhost:8080/api/documents/${this.editingDocument.id}`, this.editingDocument);\n        } else {\n          await axios.post('http://localhost:8080/api/documents', this.editingDocument);\n        }\n        this.closeModal();\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao salvar documento:', error);\n        alert('Erro ao salvar documento');\n      }\n    },\n    async deleteDocument(id) {\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\n        try {\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\n          this.loadDocuments();\n        } catch (error) {\n          console.error('Erro ao excluir documento:', error);\n          alert('Erro ao excluir documento');\n        }\n      }\n    },\n    async handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.loadDocuments();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert('Erro ao fazer upload do arquivo');\n      }\n    },\n    closeModal() {\n      this.showModal = false;\n      this.editingDocument = {\n        subject: '',\n        content: '',\n        locale: '',\n        author: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","documents","showModal","editingDocument","subject","content","locale","author","created","loadDocuments","methods","response","get","error","console","alert","showAddModal","editDocument","document","saveDocument","id","put","post","closeModal","deleteDocument","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\DocumentView.vue"],"sourcesContent":["<template>\r\n  <div class=\"document-view\">\r\n    <header class=\"document-view__header\">\r\n      <h1 class=\"document-view__title\">Documentos</h1>\r\n      <div class=\"document-view__actions\">\r\n        <button class=\"btn btn-primary\" @click=\"showAddModal\">\r\n          <i class=\"fas fa-plus\"></i>\r\n          Novo Documento\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          <i class=\"fas fa-upload\"></i>\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"visually-hidden\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"document-view__filters\">\r\n      <div class=\"filter-group\">\r\n        <div class=\"search-field\">\r\n          <i class=\"fas fa-search search-field__icon\"></i>\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"filters.search\"\r\n            placeholder=\"Pesquisar documentos...\"\r\n            class=\"form-control\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"filter-row\">\r\n        <div class=\"filter-group\">\r\n          <select v-model=\"filters.translator\" class=\"form-select\">\r\n            <option value=\"\">Todos os Tradutores</option>\r\n            <option\r\n              v-for=\"translator in translators\"\r\n              :key=\"translator.id\"\r\n              :value=\"translator.id\"\r\n            >\r\n              {{ translator.name }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n\r\n        <div class=\"filter-group\">\r\n          <select v-model=\"filters.locale\" class=\"form-select\">\r\n            <option value=\"\">Todos os Idiomas</option>\r\n            <option\r\n              v-for=\"locale in availableLocales\"\r\n              :key=\"locale\"\r\n              :value=\"locale\"\r\n            >\r\n              {{ locale }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"document-view__content\">\r\n      <div class=\"document-grid\">\r\n        <div\r\n          v-for=\"document in documents\"\r\n          :key=\"document.id\"\r\n          class=\"document-card\"\r\n        >\r\n          <div class=\"document-card__header\">\r\n            <div class=\"document-card__title-group\">\r\n              <h3 class=\"document-card__title\">{{ document.subject }}</h3>\r\n              <span class=\"document-card__locale\" :title=\"document.locale\">\r\n                <i class=\"fas fa-globe\"></i>\r\n                {{ document.locale }}\r\n              </span>\r\n            </div>\r\n            <div class=\"document-card__actions\">\r\n              <button\r\n                class=\"btn-icon\"\r\n                @click=\"editDocument(document)\"\r\n                title=\"Editar\"\r\n              >\r\n                <i class=\"fas fa-edit\"></i>\r\n              </button>\r\n              <button\r\n                class=\"btn-icon btn-icon--danger\"\r\n                @click=\"deleteDocument(document.id)\"\r\n                title=\"Excluir\"\r\n              >\r\n                <i class=\"fas fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"document-card__content\">\r\n            <p class=\"document-card__text\">{{ truncateText(document.content) }}</p>\r\n          </div>\r\n\r\n          <div class=\"document-card__footer\">\r\n            <div class=\"document-card__translator\">\r\n              <i class=\"fas fa-user-edit\"></i>\r\n              <span>{{ document.translator?.name || 'Sem tradutor' }}</span>\r\n            </div>\r\n            <div class=\"document-card__author\">\r\n              <i class=\"fas fa-user\"></i>\r\n              <span>{{ document.author }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Paginação -->\r\n      <div class=\"pagination\" v-if=\"totalPages > 1\">\r\n        <button\r\n          class=\"pagination__btn\"\r\n          :disabled=\"currentPage === 1\"\r\n          @click=\"changePage(currentPage - 1)\"\r\n        >\r\n          <i class=\"fas fa-chevron-left\"></i>\r\n        </button>\r\n        \r\n        <div class=\"pagination__numbers\">\r\n          <button\r\n            v-for=\"page in visiblePages\"\r\n            :key=\"page\"\r\n            class=\"pagination__number\"\r\n            :class=\"{ 'is-active': page === currentPage }\"\r\n            @click=\"changePage(page)\"\r\n          >\r\n            {{ page }}\r\n          </button>\r\n        </div>\r\n\r\n        <button\r\n          class=\"pagination__btn\"\r\n          :disabled=\"currentPage === totalPages\"\r\n          @click=\"changePage(currentPage + 1)\"\r\n        >\r\n          <i class=\"fas fa-chevron-right\"></i>\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal de Documento -->\r\n    <base-modal\r\n      v-if=\"showModal\"\r\n      :title=\"editingDocument.id ? 'Editar Documento' : 'Novo Documento'\"\r\n      @close=\"closeModal\"\r\n      @save=\"saveDocument\"\r\n    >\r\n      <form @submit.prevent=\"saveDocument\" class=\"document-form\">\r\n        <div class=\"form-group\">\r\n          <label>Assunto</label>\r\n          <input\r\n            v-model=\"editingDocument.subject\"\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            required\r\n          />\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label>Conteúdo</label>\r\n          <textarea\r\n            v-model=\"editingDocument.content\"\r\n            class=\"form-control\"\r\n            rows=\"5\"\r\n            required\r\n          ></textarea>\r\n        </div>\r\n\r\n        <div class=\"form-row\">\r\n          <div class=\"form-group\">\r\n            <label>Idioma</label>\r\n            <select v-model=\"editingDocument.locale\" class=\"form-select\" required>\r\n              <option value=\"\">Selecione...</option>\r\n              <option\r\n                v-for=\"locale in availableLocales\"\r\n                :key=\"locale\"\r\n                :value=\"locale\"\r\n              >\r\n                {{ locale }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label>Autor</label>\r\n            <input\r\n              v-model=\"editingDocument.author\"\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <label>Tradutor</label>\r\n          <select\r\n            v-model=\"editingDocument.translator\"\r\n            class=\"form-select\"\r\n            required\r\n          >\r\n            <option value=\"\">Selecione...</option>\r\n            <option\r\n              v-for=\"translator in translators\"\r\n              :key=\"translator.id\"\r\n              :value=\"translator\"\r\n            >\r\n              {{ translator.name }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n      </form>\r\n    </base-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'DocumentView',\r\n  data() {\r\n    return {\r\n      documents: [],\r\n      showModal: false,\r\n      editingDocument: {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.loadDocuments();\r\n  },\r\n  methods: {\r\n    async loadDocuments() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/documents');\r\n        this.documents = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar documentos:', error);\r\n        alert('Erro ao carregar documentos');\r\n      }\r\n    },\r\n    showAddModal() {\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n      this.showModal = true;\r\n    },\r\n    editDocument(document) {\r\n      this.editingDocument = { ...document };\r\n      this.showModal = true;\r\n    },\r\n    async saveDocument() {\r\n      try {\r\n        if (this.editingDocument.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/documents/${this.editingDocument.id}`,\r\n            this.editingDocument\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/documents',\r\n            this.editingDocument\r\n          );\r\n        }\r\n        this.closeModal();\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar documento:', error);\r\n        alert('Erro ao salvar documento');\r\n      }\r\n    },\r\n    async deleteDocument(id) {\r\n      if (confirm('Tem certeza que deseja excluir este documento?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/documents/${id}`);\r\n          this.loadDocuments();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir documento:', error);\r\n          alert('Erro ao excluir documento');\r\n        }\r\n      }\r\n    },\r\n    async handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/documents/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        this.loadDocuments();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert('Erro ao fazer upload do arquivo');\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.editingDocument = {\r\n        subject: '',\r\n        content: '',\r\n        locale: '',\r\n        author: ''\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.document-view {\r\n  padding: var(--spacing-4);\r\n\r\n  &__header {\r\n    @include flex(row, space-between, center);\r\n    margin-bottom: var(--spacing-4);\r\n\r\n    @media (max-width: 576px) {\r\n      flex-direction: column;\r\n      gap: var(--spacing-3);\r\n    }\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    color: var(--text-primary);\r\n    font-size: 1.75rem;\r\n  }\r\n\r\n  &__actions {\r\n    @include flex(row, flex-end, center);\r\n    gap: var(--spacing-2);\r\n\r\n    @media (max-width: 576px) {\r\n      width: 100%;\r\n      justify-content: stretch;\r\n\r\n      .btn {\r\n        flex: 1;\r\n      }\r\n    }\r\n  }\r\n\r\n  &__filters {\r\n    margin-bottom: var(--spacing-4);\r\n    \r\n    .filter-row {\r\n      @include flex(row, flex-start, center);\r\n      gap: var(--spacing-3);\r\n      margin-top: var(--spacing-3);\r\n\r\n      @media (max-width: 768px) {\r\n        flex-direction: column;\r\n        width: 100%;\r\n\r\n        .filter-group {\r\n          width: 100%;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.search-field {\r\n  position: relative;\r\n\r\n  &__icon {\r\n    position: absolute;\r\n    left: var(--spacing-3);\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    color: var(--gray-500);\r\n  }\r\n\r\n  .form-control {\r\n    padding-left: calc(var(--spacing-3) * 2 + 16px);\r\n  }\r\n}\r\n\r\n.document-grid {\r\n  display: grid;\r\n  gap: var(--spacing-3);\r\n\r\n  @include respond-to('sm') {\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n\r\n  @include respond-to('lg') {\r\n    grid-template-columns: repeat(3, 1fr);\r\n  }\r\n\r\n  @include respond-to('xl') {\r\n    grid-template-columns: repeat(4, 1fr);\r\n  }\r\n}\r\n\r\n.document-card {\r\n  background-color: white;\r\n  border-radius: var(--border-radius);\r\n  box-shadow: var(--shadow-sm);\r\n  padding: var(--spacing-3);\r\n  @include smooth-transition(transform);\r\n\r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    box-shadow: var(--shadow);\r\n  }\r\n\r\n  &__header {\r\n    @include flex(row, space-between, flex-start);\r\n    margin-bottom: var(--spacing-3);\r\n  }\r\n\r\n  &__title-group {\r\n    flex: 1;\r\n    min-width: 0;\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    font-size: 1.1rem;\r\n    color: var(--text-primary);\r\n    margin-bottom: var(--spacing-1);\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n    white-space: nowrap;\r\n  }