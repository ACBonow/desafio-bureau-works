iv class=\"search-field\">\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchTerm\"\r\n          placeholder=\"Pesquisar tradutores...\"\r\n          class=\"form-control\"\r\n        />\r\n      </div>\r\n      \r\n      <div class=\"filter-group\">\r\n        <select v-model=\"selectedLanguage\" class=\"form-select\">\r\n          <option value=\"\">Todos os idiomas</option>\r\n          <option v-for=\"lang in languages\" :key=\"lang\" :value=\"lang\">\r\n            {{ lang }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"translator-view__content\">\r\n      <div class=\"translator-grid\">\r\n        <div \r\n          v-for=\"translator in filteredTranslators\" \r\n          :key=\"translator.id\"\r\n          class=\"translator-card\"\r\n        >\r\n          <div class=\"translator-card__header\">\r\n            <h3 class=\"translator-card__name\">{{ translator.name }}</h3>\r\n            <div class=\"translator-card__actions\">\r\n              <button \r\n                class=\"btn-icon\"\r\n                @click=\"editTranslator(translator)\"\r\n                title=\"Editar\"\r\n              >\r\n                <i class=\"fas fa-edit\"></i>\r\n              </button>\r\n              <button \r\n                class=\"btn-icon btn-icon--danger\"\r\n                @click=\"deleteTranslator(translator.id)\"\r\n                title=\"Excluir\"\r\n              >\r\n                <i class=\"fas fa-trash\"></i>\r\n              </button>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"translator-card__info\">\r\n            <p><i class=\"fas fa-envelope\"></i> {{ translator.email }}</p>\r\n            <p>\r\n              <i class=\"fas fa-language\"></i> \r\n              {{ translator.sourceLanguage }} â†’ {{ translator.targetLanguage }}\r\n            </p>\r\n          </div>\r\n          \r\n          <div class=\"translator-card__stats\">\r\n            <div class=\"stat\">\r\n              <span class=\"stat__value\">{{ translator.documents?.length || 0 }}</span>\r\n              <span class=\"stat__label\">Documentos</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n.translator-view {\r\n  padding: var(--spacing-4);\r\n  \r\n  &__header {\r\n    @include flex(row, space-between, center);\r\n    margin-bottom: var(--spacing-4);\r\n    \r\n    @media (max-width: 576px) {\r\n      flex-direction: column;\r\n      gap: var(--spacing-3);\r\n    }\r\n  }\r\n  \r\n  &__title {\r\n    margin: 0;\r\n    color: var(--text-primary);\r\n  }\r\n  \r\n  &__actions {\r\n    @include flex(row, flex-end, center);\r\n    gap: var(--spacing-2);\r\n    \r\n    @media (max-width: 576px) {\r\n      width: 100%;\r\n      justify-content: stretch;\r\n      \r\n      .btn {\r\n        flex: 1;\r\n      }\r\n    }\r\n  }\r\n  \r\n  &__filters {\r\n    @include flex(row, space-between, center);\r\n    gap: var(--spacing-3);\r\n    margin-bottom: var(--spacing-4);\r\n    \r\n    @media (max-width: 768px) {\r\n      flex-direction: column;\r\n      \r\n      .search-field,\r\n      .filter-group {\r\n        width: 100%;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.translator-grid {\r\n  display: grid;\r\n  gap: var(--spacing-3);\r\n  \r\n  @include respond-to('sm') {\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n  \r\n  @include respond-to('lg') {\r\n    grid-template-columns: repeat(3, 1fr);\r\n  }\r\n  \r\n  @include respond-to('xl') {\r\n    grid-template-columns: repeat(4, 1fr);\r\n  }\r\n}\r\n\r\n.translator-card {\r\n  background-color: white;\r\n  border-radius: var(--border-radius);\r\n  box-shadow: var(--shadow-sm);\r\n  padding: var(--spacing-3);\r\n  @include smooth-transition(transform);\r\n  \r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    box-shadow: var(--shadow);\r\n  }\r\n  \r\n  &__header {\r\n    @include flex(row, space-between, center);\r\n    margin-bottom: var(--spacing-3);\r\n  }\r\n  \r\n  &__name {\r\n    margin: 0;\r\n    font-size: 1.25rem;\r\n    color: var(--text-primary);\r\n  }\r\n  \r\n  &__actions {\r\n    @include flex(row, flex-end, center);\r\n    gap: var(--spacing-2);\r\n  }\r\n  \r\n  &__info {\r\n    color: var(--text-secondary);\r\n    \r\n    p {\r\n      margin: var(--spacing-2) 0;\r\n      @include flex(row, flex-start, center);\r\n      gap: var(--spacing-2);\r\n      \r\n      i {\r\n        color: var(--primary-color);\r\n      }\r\n    }\r\n  }\r\n  \r\n  &__stats {\r\n    margin-top: var(--spacing-3);\r\n    padding-top: var(--spacing-3);\r\n    border-top: 1px solid var(--gray-200);\r\n    \r\n    .stat {\r\n      text-align: center;\r\n      \r\n      &__value {\r\n        display: block;\r\n        font-size: 1.5rem;\r\n        font-weight: bold;\r\n        color: var(--primary-color);\r\n      }\r\n      \r\n      &__label {\r\n        color: var(--text-secondary);\r\n        font-size: 0.875rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.btn-icon {\r\n  background: none;\r\n  border: none;\r\n  padding: var(--spacing-2);\r\n  border-radius: var(--border-radius);\r\n  cursor: pointer;\r\n  color: var(--gray-600);\r\n  @include smooth-transition(all);\r\n  \r\n  &:hover {\r\n    background-color: var(--gray-100);\r\n    color: var(--primary-color);\r\n  }\r\n  \r\n  &--danger:hover {\r\n    background-color: var(--danger-color);\r\n    color: white;\r\n  }\r\n}\r\n</style>\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'TranslatorView',\r\n  data() {\r\n    return {\r\n      translators: [],\r\n      showModal: false,\r\n      editingTranslator: {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.loadTranslators();\r\n  },\r\n  methods: {\r\n    async loadTranslators() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/translators');\r\n        this.translators = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar tradutores:', error);\r\n        alert('Erro ao carregar tradutores');\r\n      }\r\n    },\r\n    showAddModal() {\r\n      this.editingTranslator = {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      };\r\n      this.showModal = true;\r\n    },\r\n    editTranslator(translator) {\r\n      this.editingTranslator = { ...translator };\r\n      this.showModal = true;\r\n    },\r\n    async saveTranslator() {\r\n      try {\r\n        if (this.editingTranslator.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/translators/${this.editingTranslator.id}`,\r\n            this.editingTranslator\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/translators',\r\n            this.editingTranslator\r\n          );\r\n        }\r\n        this.closeModal();\r\n        this.loadTranslators();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar tradutor:', error);\r\n        alert('Erro ao salvar tradutor');\r\n      }\r\n    },\r\n    async deleteTranslator(id) {\r\n      if (confirm('Tem certeza que deseja excluir este tradutor?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/translators/${id}`);\r\n          this.loadTranslators();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir tradutor:', error);\r\n          alert('Erro ao excluir tradutor');\r\n        }\r\n      }\r\n    },\r\n    async handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/ap