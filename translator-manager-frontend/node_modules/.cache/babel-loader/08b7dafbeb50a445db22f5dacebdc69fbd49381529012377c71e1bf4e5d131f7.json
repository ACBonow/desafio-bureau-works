{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'TranslatorView',\n  data() {\n    return {\n      translators: [],\n      showModal: false,\n      editingTranslator: {\n        name: '',\n        email: '',\n        sourceLanguage: '',\n        targetLanguage: ''\n      }\n    };\n  },\n  created() {\n    this.loadTranslators();\n  },\n  methods: {\n    async loadTranslators() {\n      try {\n        const response = await axios.get('http://localhost:8080/api/translators');\n        this.translators = response.data;\n      } catch (error) {\n        console.error('Erro ao carregar tradutores:', error);\n        alert('Erro ao carregar tradutores');\n      }\n    },\n    showAddModal() {\n      this.editingTranslator = {\n        name: '',\n        email: '',\n        sourceLanguage: '',\n        targetLanguage: ''\n      };\n      this.showModal = true;\n    },\n    editTranslator(translator) {\n      this.editingTranslator = {\n        ...translator\n      };\n      this.showModal = true;\n    },\n    async saveTranslator() {\n      try {\n        if (this.editingTranslator.id) {\n          await axios.put(`http://localhost:8080/api/translators/${this.editingTranslator.id}`, this.editingTranslator);\n        } else {\n          await axios.post('http://localhost:8080/api/translators', this.editingTranslator);\n        }\n        this.closeModal();\n        this.loadTranslators();\n      } catch (error) {\n        console.error('Erro ao salvar tradutor:', error);\n        alert('Erro ao salvar tradutor');\n      }\n    },\n    async deleteTranslator(id) {\n      if (confirm('Tem certeza que deseja excluir este tradutor?')) {\n        try {\n          await axios.delete(`http://localhost:8080/api/translators/${id}`);\n          this.loadTranslators();\n        } catch (error) {\n          console.error('Erro ao excluir tradutor:', error);\n          alert('Erro ao excluir tradutor');\n        }\n      }\n    },\n    async handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      const formData = new FormData();\n      formData.append('file', file);\n      try {\n        await axios.post('http://localhost:8080/api/translators/upload', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.loadTranslators();\n      } catch (error) {\n        console.error('Erro ao fazer upload do arquivo:', error);\n        alert('Erro ao fazer upload do arquivo');\n      }\n    },\n    closeModal() {\n      this.showModal = false;\n      this.editingTranslator = {\n        name: '',\n        email: '',\n        sourceLanguage: '',\n        targetLanguage: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","translators","showModal","editingTranslator","email","sourceLanguage","targetLanguage","created","loadTranslators","methods","response","get","error","console","alert","showAddModal","editTranslator","translator","saveTranslator","id","put","post","closeModal","deleteTranslator","confirm","delete","handleFileUpload","event","file","target","files","formData","FormData","append","headers"],"sources":["C:\\Projetos\\translator-manager-frontend\\src\\views\\TranslatorView.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"d-flex justify-content-between align-items-center mb-4\">\r\n      <h2>Gerenciamento de Tradutores</h2>\r\n      <div>\r\n        <button class=\"btn btn-primary me-2\" @click=\"showAddModal\">\r\n          Novo Tradutor\r\n        </button>\r\n        <label class=\"btn btn-secondary\">\r\n          Upload CSV\r\n          <input\r\n            type=\"file\"\r\n            accept=\".csv\"\r\n            class=\"d-none\"\r\n            @change=\"handleFileUpload\"\r\n          />\r\n        </label>\r\n      </div>\r\n    </div>\r\n\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Nome</th>\r\n          <th>Email</th>\r\n          <th>Idioma Origem</th>\r\n          <th>Idioma Destino</th>\r\n          <th>Ações</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"translator in translators\" :key=\"translator.id\">\r\n          <td>{{ translator.name }}</td>\r\n          <td>{{ translator.email }}</td>\r\n          <td>{{ translator.sourceLanguage }}</td>\r\n          <td>{{ translator.targetLanguage }}</td>\r\n          <td>\r\n            <button\r\n              class=\"btn btn-sm btn-primary me-2\"\r\n              @click=\"editTranslator(translator)\"\r\n            >\r\n              Editar\r\n            </button>\r\n            <button\r\n              class=\"btn btn-sm btn-danger\"\r\n              @click=\"deleteTranslator(translator.id)\"\r\n            >\r\n              Excluir\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Modal para Adicionar/Editar -->\r\n    <div class=\"modal\" v-if=\"showModal\" tabindex=\"-1\" style=\"display: block\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">\r\n              {{ editingTranslator.id ? 'Editar' : 'Novo' }} Tradutor\r\n            </h5>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn-close\"\r\n              @click=\"closeModal\"\r\n            ></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <form @submit.prevent=\"saveTranslator\">\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Nome</label>\r\n                <input\r\n                  v-model=\"editingTranslator.name\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Email</label>\r\n                <input\r\n                  v-model=\"editingTranslator.email\"\r\n                  type=\"email\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Idioma Origem</label>\r\n                <input\r\n                  v-model=\"editingTranslator.sourceLanguage\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">Idioma Destino</label>\r\n                <input\r\n                  v-model=\"editingTranslator.targetLanguage\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"modal-footer\">\r\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeModal\">\r\n                  Cancelar\r\n                </button>\r\n                <button type=\"submit\" class=\"btn btn-primary\">Salvar</button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'TranslatorView',\r\n  data() {\r\n    return {\r\n      translators: [],\r\n      showModal: false,\r\n      editingTranslator: {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.loadTranslators();\r\n  },\r\n  methods: {\r\n    async loadTranslators() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8080/api/translators');\r\n        this.translators = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao carregar tradutores:', error);\r\n        alert('Erro ao carregar tradutores');\r\n      }\r\n    },\r\n    showAddModal() {\r\n      this.editingTranslator = {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      };\r\n      this.showModal = true;\r\n    },\r\n    editTranslator(translator) {\r\n      this.editingTranslator = { ...translator };\r\n      this.showModal = true;\r\n    },\r\n    async saveTranslator() {\r\n      try {\r\n        if (this.editingTranslator.id) {\r\n          await axios.put(\r\n            `http://localhost:8080/api/translators/${this.editingTranslator.id}`,\r\n            this.editingTranslator\r\n          );\r\n        } else {\r\n          await axios.post(\r\n            'http://localhost:8080/api/translators',\r\n            this.editingTranslator\r\n          );\r\n        }\r\n        this.closeModal();\r\n        this.loadTranslators();\r\n      } catch (error) {\r\n        console.error('Erro ao salvar tradutor:', error);\r\n        alert('Erro ao salvar tradutor');\r\n      }\r\n    },\r\n    async deleteTranslator(id) {\r\n      if (confirm('Tem certeza que deseja excluir este tradutor?')) {\r\n        try {\r\n          await axios.delete(`http://localhost:8080/api/translators/${id}`);\r\n          this.loadTranslators();\r\n        } catch (error) {\r\n          console.error('Erro ao excluir tradutor:', error);\r\n          alert('Erro ao excluir tradutor');\r\n        }\r\n      }\r\n    },\r\n    async handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (!file) return;\r\n\r\n      const formData = new FormData();\r\n      formData.append('file', file);\r\n\r\n      try {\r\n        await axios.post('http://localhost:8080/api/translators/upload', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        this.loadTranslators();\r\n      } catch (error) {\r\n        console.error('Erro ao fazer upload do arquivo:', error);\r\n        alert('Erro ao fazer upload do arquivo');\r\n      }\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.editingTranslator = {\r\n        name: '',\r\n        email: '',\r\n        sourceLanguage: '',\r\n        targetLanguage: ''\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>"],"mappings":"AAyHA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MAChBC,iBAAiB,EAAE;QACjBJ,IAAI,EAAE,EAAE;QACRK,KAAK,EAAE,EAAE;QACTC,cAAc,EAAE,EAAE;QAClBC,cAAc,EAAE;MAClB;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,uCAAuC,CAAC;QACzE,IAAI,CAACV,WAAU,GAAIS,QAAQ,CAACV,IAAI;MAClC,EAAE,OAAOY,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpDE,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACZ,iBAAgB,GAAI;QACvBJ,IAAI,EAAE,EAAE;QACRK,KAAK,EAAE,EAAE;QACTC,cAAc,EAAE,EAAE;QAClBC,cAAc,EAAE;MAClB,CAAC;MACD,IAAI,CAACJ,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDc,cAAcA,CAACC,UAAU,EAAE;MACzB,IAAI,CAACd,iBAAgB,GAAI;QAAE,GAAGc;MAAW,CAAC;MAC1C,IAAI,CAACf,SAAQ,GAAI,IAAI;IACvB,CAAC;IACD,MAAMgB,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,IAAI,IAAI,CAACf,iBAAiB,CAACgB,EAAE,EAAE;UAC7B,MAAMrB,KAAK,CAACsB,GAAG,CACb,yCAAyC,IAAI,CAACjB,iBAAiB,CAACgB,EAAE,EAAE,EACpE,IAAI,CAAChB,iBACP,CAAC;QACH,OAAO;UACL,MAAML,KAAK,CAACuB,IAAI,CACd,uCAAuC,EACvC,IAAI,CAAClB,iBACP,CAAC;QACH;QACA,IAAI,CAACmB,UAAU,CAAC,CAAC;QACjB,IAAI,CAACd,eAAe,CAAC,CAAC;MACxB,EAAE,OAAOI,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;QAChDE,KAAK,CAAC,yBAAyB,CAAC;MAClC;IACF,CAAC;IACD,MAAMS,gBAAgBA,CAACJ,EAAE,EAAE;MACzB,IAAIK,OAAO,CAAC,+CAA+C,CAAC,EAAE;QAC5D,IAAI;UACF,MAAM1B,KAAK,CAAC2B,MAAM,CAAC,yCAAyCN,EAAE,EAAE,CAAC;UACjE,IAAI,CAACX,eAAe,C